import javax.swing.JOptionPane;

public class ventana {

	public static void main(String[] args) {
		int edad = Integer.parseInt(JOptionPane.showInputDialog("Ingrese la edad"));
		
		if(edad >= 18) {
			if(edad == 25) {
				System.out.println(JOptionPane.showConfirmDialog(null, "Ud es mayor de edad y tiene 25 a√±os"));
			} else {
				System.out.println(JOptionPane.showConfirmDialog(null,"Ud es mayor de edad"));
			}
		}else {
			System.out.println(JOptionPane.showConfirmDialog(null, "Ud es menor de edad"));
		}
		
	}

}
